            <string>///Action switch
if dirBlock == directionBlockEnum.YY {
    if (dist_pos_y &gt; y) {
        if (abs(y - dist_pos_y) &lt;= actionSpeed) {
            y = dist_pos_y;
        } else {
            y += actionSpeed;
        }
    }
    else if (dist_pos_y &lt; y) {
        if (abs(y - dist_pos_y) &lt;= actionSpeed) {
            y = dist_pos_y;
        } else {
            y -= actionSpeed;
        }
    }
    
}
else if dirBlock == directionBlockEnum.XX {
    if (dist_pos_x &gt; x) {
        if (abs(x - dist_pos_x) &lt;= actionSpeed) {
            x = dist_pos_x;
        } else {
            x += actionSpeed;
        }
    }
    else if (dist_pos_x &lt; x) {
        if (abs(x - dist_pos_x) &lt;= actionSpeed) {
            x = dist_pos_x;
        } else {
            x -= actionSpeed;
        }
    }
    
}


if (dist_pos_y == y and dist_pos_x == x) {
    dirBlock = directionBlockEnum.NONE;
    state = blockstate.STATIC;
    dist_pos_x = -1;
    dist_pos_y = -1;
}
